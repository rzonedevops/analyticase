{
  "simulation_name": "Enhanced AnalytiCase Simulation Run",
  "version": "2.0",
  "timestamp": "2025-10-22",
  "description": "Comprehensive simulation with enhanced models, legal framework integration, and advanced analytics",
  
  "agent_based": {
    "num_investigators": 8,
    "num_attorneys": 12,
    "num_judges": 5,
    "num_steps": 200,
    "enable_learning": true,
    "enable_collaboration": true,
    "initial_expertise_range": [0.4, 0.8],
    "learning_rate": 0.01
  },
  
  "discrete_event": {
    "num_cases": 50,
    "simulation_time": 365,
    "case_arrival_rate": 0.3,
    "enable_priority_queue": true,
    "resource_constraints": true
  },
  
  "system_dynamics": {
    "simulation_duration": 730,
    "time_step": 1,
    "initial_case_backlog": 100,
    "judge_capacity": 50,
    "attorney_capacity": 80,
    "feedback_loops_enabled": true
  },
  
  "hyper_gnn": {
    "num_layers": 3,
    "embedding_dim": 128,
    "aggregation_type": "attention",
    "learning_rate": 0.001,
    "num_epochs": 100,
    "enable_temporal_analysis": true,
    "enable_community_detection": true
  },
  
  "case_llm": {
    "model": "gpt-4.1-mini",
    "enable_rag": true,
    "max_tokens": 2000,
    "temperature": 0.7,
    "enable_legal_reasoning": true,
    "use_legal_frameworks": true
  },
  
  "integration": {
    "enable_cross_model_validation": true,
    "enable_ensemble_predictions": true,
    "sync_with_database": true,
    "generate_visualizations": true
  },
  
  "output": {
    "save_detailed_logs": true,
    "save_metrics": true,
    "save_visualizations": true,
    "generate_report": true,
    "export_format": ["json", "csv", "markdown"]
  }
}

